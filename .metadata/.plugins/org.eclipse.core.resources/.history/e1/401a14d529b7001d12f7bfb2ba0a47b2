package com.control.veterinaria.dao.interfaces;

import java.util.List;

import com.control.veterinaria.model.Colaborador;

public interface UsuarioDao {
	public Colaborador save(Colaborador usuario);
	public Colaborador update(Colaborador usuario);
	public void delete(Colaborador usuario);
	public List<Colaborador> findAll();
	public Colaborador findById(Integer id);
	public boolean existsById(Integer id);
}
